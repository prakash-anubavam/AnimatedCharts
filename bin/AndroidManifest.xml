<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.steveinflow.animcharts"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_bw"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.steveinflow.animatedcharts.sample.ChartFragment"
            android:label="@string/app_name" >
            
        </activity>
        <activity
            android:name="com.steveinflow.animatedcharts.sample.ItemListFragment"
            android:label="@string/title_activity_item_list_fragment" >
        </activity>
        <activity
            android:name="com.steveinflow.animatedcharts.sample.MainActivity"
            android:label="Animated Charts" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
